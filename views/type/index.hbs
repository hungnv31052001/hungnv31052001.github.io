<div class="container col-md-6">
   <table class="table table-striped">
      <tr>
         <th>Type Name</th>
         <th>Type Quantity</th>
         <th>Menu</th>
      </tr>
      {{#each typeList }}
      <tr>
         <td>{{ name }}</td>
         <td>{{ quantity }}</td>
         <td>
            <a class="btn btn-success" href="/type/edit/{{ _id }}">Edit</a>
            <a class="btn btn-success" href="/type/delete/{{ _id }}" onclick="return confirm('Do you want to delete this type ?');">Delete</a>
         </td>
      </tr>
      {{/each}}
   </table>
</div>